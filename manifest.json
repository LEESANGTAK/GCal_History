{
  "manifest_version": 3,
  "name": "구글 캘린더 제목 자동완성 (G-Cal History)",
  "version": "1.0",
  "description": "이전에 입력했던 일정 제목을 기억하여 자동으로 추천해줍니다.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://calendar.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}